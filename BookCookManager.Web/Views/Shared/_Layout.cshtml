@using System.Web.Optimization;

<!DOCTYPE html>
<html lang="en">
<head>

    <title>Soft Landing Page</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="team" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">


    @Styles.Render("~/bundles/bootstrap/css")
    @Styles.Render("~/bundles/owlcarousel/css")
    @Styles.Render("~/bundles/owlthemedefault/css")
    @Styles.Render("~/bundles/fontawesome/css")

    <!-- MAIN CSS -->
    @Styles.Render("~/bundles/tooplatestyle/css")

</head>
<body>

    <section class="navbar custom-navbar navbar-fixed-top" role="navigation">
        @Html.Partial("_Menu")
    </section>

    <section id="about" data-stellar-background-ratio="0.5">
        @Html.Partial("_About")
    </section>

    <section id="contact" data-stellar-background-ratio="0.5">
        @Html.Partial("_Contact")
    </section>

    <footer id="footer" data-stellar-background-ratio="0.5">
        @Html.Partial("_Footer")
    </footer>

</body>

    @Scripts.Render("~/bundles/jquery/js")

    @Scripts.Render("~/bundles/bootstrap/js")
    @Scripts.Render("~/bundles/jquerystellar/js")
    @Scripts.Render("~/bundles/owlcarousel/js") 

    @Scripts.Render("~/bundles/smoothscroll/js")
    @Scripts.Render("~/bundles/custom/js")


</html>